{
  "name": "sendit-frontend-reactjs",
  "version": "1.0.0",
  "description": "A courier service that helps users deliver parcels to different destinations",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./server.js",
    "build": "webpack --mode production",
    "test": "jest  --coverage --coverageReporters=text-lcov | coveralls",
    "start:dev": "webpack-dev-server --mode development --open --hot",
    "server": "node ./server.js",
    "start:prod": "nodemon --exec babel-node server/app.js"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ODINAKACHUKWU/sendit-frontend-reactjs.git"
  },
  "author": "Odinakachukwu Ezeobika",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ODINAKACHUKWU/sendit-frontend-reactjs/issues"
  },
  "homepage": "https://github.com/ODINAKACHUKWU/sendit-frontend-reactjs#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "is-empty": "^1.2.0",
    "jwt-decode": "^2.2.0",
    "keymirror": "^0.1.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-md-spinner": "^0.3.0",
    "react-modal": "^3.8.1",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "toastr": "^2.1.4",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "jest-transform-stub": "^2.0.0",
    "react-router-dom": "^4.3.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
